{
  "name": "pan-domain-lambda",
  "version": "1.0.0",
  "description": "Custom lambda authenticator using pan-domain for API Gateway",
  "projectName": "cms-fronts::pan-domain-lamda",
  "private": true,
  "license": "Apache-2.0",
  "main": "src/index.js",
  "cloudformation": false,
  "buildDir": "tmp/lambda",
  "isAwsLambda": true,
  "riffraffFile": "./riff-raff.yaml",
  "scripts": {
    "pretest": "gulp lint",
    "test": "gulp compile && tap test",
    "deploy": "gulp deploy && VERBOSE=true ARTEFACT_PATH=$PWD riffraff-artefact",
    "watch": "nodemon --watch src --watch test --exec 'npm test'"
  },
  "devDependencies": {
    "aws-sdk": "^2.3.17",
    "babel-preset-es2015-rollup": "^1.1.1",
    "exec-chainable": "0.0.3",
    "gulp": "^3.9.1",
    "gulp-eslint": "^2.0.0",
    "node-riffraff-artefact": "^2.0.1",
    "rollup": "^0.27.0",
    "rollup-plugin-babel": "^2.5.0",
    "rollup-plugin-commonjs": "^2.2.1",
    "rollup-plugin-node-resolve": "^1.5.0",
    "tap": "^5.7.2"
  },
  "peerDependencies": {
    "aws-sdk": "2.x"
  },
  "dependencies": {
    "cookie": "^0.3.1",
    "pan-domain-public-keys": "^1.2.0",
    "pan-domain-validate-user": "^1.0.2"
  }
}
